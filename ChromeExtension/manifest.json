{
    "manifest_version": 2,

    "name": "Geniuslink",
    "description": "Save Amazon Links fast!",
    "version": "0.1",

    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "apikeys.html"
    },
    "icons": {
        "16": "images/icon.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
    },

    "background": {
        "scripts": ["js/background.js", "js/jquery.min.js"],
        "persistent": true
    },


    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "contextMenus",
        "tabs",
        "https://api.geni.us/",
        "storage"
    ]
}
